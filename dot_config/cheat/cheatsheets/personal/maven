# Start new project
mvn archetype:generate -DgroupId=com.deploymenteveryday.webcrawler -DartifactId=webcrawler -DarchetypeArtifactId=maven-archetype-quickstart -DinteractiveMode=false

# Download artifact
mvn dependency:get -Dartifact=group-id:artefact-id:version
